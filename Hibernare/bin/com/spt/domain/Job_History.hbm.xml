<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.spt.domain.Job_History" table="JOB_HISTORY">
		<id name="employeeID" column="EMPLOYEE_ID" type="java.lang.Integer"></id>
		<property name="startDate" column="START_DATE" type="java.util.Date"/>
		<property name="endDate" column="END_DATE" type="java.util.Date"/>
		<property name="jobID" column="JOB_ID" type="string"/>
		<property name="departmentID" column="DEPARTMENT_ID" type="java.lang.Integer"/>
	</class>

</hibernate-mapping>